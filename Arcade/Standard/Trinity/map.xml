<map proto="1.5.0">
<name>Trinity</name>
<version>1.0.4</version>
<objective>Eliminate the other team before the time is up.</objective>
<gamemode>arcade</gamemode>
<gamemode>br</gamemode>
<authors>
    <author uuid="bf331953-4f92-43ee-8abc-7544b8234936"/> <!-- ParaSpider -->
    <author uuid="0b8f95fa-0f40-4f28-9fd0-b8e243bb6800"/> <!-- Slicorn -->
    <author uuid="3cce0080-9faa-48f9-82cf-64410305822a"/> <!-- Sethskye -->
</authors>
<contributors>
    <contributor uuid="e71a0925-c6f9-462d-b864-bf3b652d152f"/> <!-- ScriptingCookies -->
    <contributor uuid="f866f386-3d9d-493c-8de8-87b493cab8be"/> <!-- alexanderjoe10 -->
</contributors>
<teams>
    <team id="red" color="dark red" max="15" max-overfill="18">Red</team>
    <team id="blue" color="blue" max="15" max-overfill="18">Blue</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" lore="`91-Hit Kill!" material="diamond sword"/>
        <item slot="1" enchantment="arrow damage:10" lore="`91-Shot Kill!" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet locked="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" team-color="true" material="leather leggings"/>
        <boots locked="true" team-color="true" material="leather boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5" amplifier="4">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="spawn-kit">
        <region>
            <cuboid min="7,3,3" max="5,3,5"/>
        </region>
    </spawn>
    <spawn team="blue" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="7,3,127" max="5,3,125"/>
        </region>
    </spawn>
    <default yaw="90">
        <region>
            <cuboid min="47,64,66" max="49,64,64"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>5m</time>
<blitz>
    <lives>2</lives>
</blitz>
<rage/>
<toolrepair>
    <tool>diamond sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn auto="true"/>
</map>
