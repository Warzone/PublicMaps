<time result="defenders">6m</time>
<gamerules>
    <naturalRegeneration>false</naturalRegeneration>
</gamerules>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
</map>
