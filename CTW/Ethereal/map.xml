<map proto="1.5.0">
<name>Ethereal</name>
<version>1.3.3</version>
<objective>Capture the wool and return it to your victory monument.</objective>
<gamemode>ctw</gamemode>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="65722be1-29fd-4e7d-a47d-94ed616ed880"/> <!-- KroestV2 -->
    <author uuid="38ab65dd-95f9-406f-917c-6accc995421f"/> <!-- GoldenOre -->
</authors>
<teams>
    <team id="pink" color="light purple" max="32" max-overfill="36">Pink</team>
    <team id="yellow" color="yellow" max="32" max-overfill="36">Yellow</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" enchantment="arrow infinite" material="bow"/>
        <item slot="2" unbreakable="true" enchantment="durability:4;dig speed" material="iron pickaxe"/>
        <item slot="3" unbreakable="true" enchantment="durability:4;dig speed" material="iron axe"/>
        <item slot="4" amount="64" damage="2" material="wood"/>
        <item slot="5" amount="64" material="glass"/>
        <item slot="6" material="boat"/>
        <item slot="8" amount="1" material="golden apple"/>
        <item slot="28" material="arrow"/>
        <chestplate team-color="true" unbreakable="true" material="leather chestplate"/>
        <leggings enchantment="protection projectile:2" unbreakable="true" material="chainmail leggings"/>
        <boots team-color="true" unbreakable="true" material="leather boots"/>
        <effect duration="5" amplifier="3">heal</effect>
    </kit>
</kits>
<spawns>
    <spawn team="pink" kit="spawn-kit" yaw="90">
        <regions>
            <point>158.5,30,-26.5</point>
            <point>158.5,30,27.5</point>
        </regions>
    </spawn>
    <spawn team="yellow" kit="spawn-kit" yaw="-90">
        <regions>
            <point>-157.5,30,27.5</point>
            <point>-157.5,30,-26.5</point>
        </regions>
    </spawn>
    <default>
        <region>
            <point>0.5,105.5,0.5</point>
        </region>
    </default>
</spawns>
<filters>
    <team id="yellow-team">yellow</team>
    <team id="pink-team">pink</team>
    <material id="webs">web</material>
    <material id="beacons">beacon</material>
    <material id="only-iron">iron block</material>
    <not id="wool-room-blocks">
        <any>
            <material>chest</material>
            <material>chest</material>
            <material>workbench</material>
            <material>beacon</material>
        </any>
    </not>
</filters>
<regions>
    <negative id="void-area">
        <union id="bridgezones">
            <rectangle min="127,16" max="95,-15"/>
            <rectangle min="-126,16" max="-94,-15"/>
            <!-- mid zones -->
            <rectangle min="-12,-38" max="13,-15"/>
            <rectangle min="-12,39" max="13,16"/>
        </union>
    </negative>
    <union id="team-bases">
        <union id="yellow-base">
            <rectangle min="-161,16" max="-149,39"/>
            <rectangle min="-161,-38" max="-149,-15"/>
        </union>
        <union id="pink-base">
            <rectangle min="150,-38" max="162,-15"/>
            <rectangle min="162,16" max="150,39"/>
        </union>
    </union>
    <rectangle id="pink-wool" min="154,-5" max="172,6"/>
    <rectangle id="yellow-wool" min="-153,6" max="-171,-5"/>
    <union id="iron">
        <union id="yellow-iron">
            <cuboid min="93,23,39" max="-144,30,-39"/>
        </union>
        <mirror id="pink-iron" region="yellow-iron" normal="-1,0,0" origin="0.5,24,0.5"/> 
    </union>
    <!-- Applied -->
    <apply region="pink-base" enter="pink" message="You may not enter the enemy's spawn area!"/>
    <apply region="yellow-base" enter="yellow" message="You may not enter the enemy's spawn area!"/>
    <apply block="never">
        <region>
            <region id="team-bases"/>
            <cuboid min="99,25,-3" max="94,20,4"/>
            <cuboid min="-98,25,4" max="-93,20,-3"/>
        </region>
    </apply>
    <apply region="pink-wool" enter="yellow" message="You may not enter your own wool room!"/>
    <apply region="yellow-wool" enter="pink" message="You may not enter your own wool room!"/>
    <apply region="pink-wool" block="all(wool-room-blocks,deny(pink-team))" use="all(yellow-team,deny(beacons))"/>
    <apply region="yellow-wool" block="all(wool-room-blocks,deny(yellow-team))" use="all(pink-team,deny(beacons))"/>
    <apply block="never" message="You may not modify the spawn!">
        <region>
            <above y="90"/>
        </region>
    </apply>
    <apply region="bridgezones" block="deny(webs)"/>
    <apply region="void-area" block="deny(void)" message="You may not build outside the map area!"/>
</regions>
<wools>
    <wool team="pink" color="yellow" location="-163.5,31,0.5">
        <monument>
            <block>96,22,0</block>
        </monument>
    </wool>
    <wool team="yellow" color="pink" location="164.5,31,0.5">
        <monument>
            <block>-96,22,0</block>
        </monument>
    </wool>
</wools>
<renewables>
    <renewable region="iron" renew-filter="only-iron" interval="8" grow="false" particles="true" sound="true"/>
</renewables>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>seeds</item>
    <item>red rose</item>
    <item>yellow flower</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>string</item>
    <item>web</item>
</itemremove>
<kill-reward>
    <item amount="16" material="wood"/>
    <item amount="16" material="glass"/>
</kill-reward>
<maxbuildheight>45</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>