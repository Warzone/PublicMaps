<block-drops>
    <rule wrong-tool="false">
        <region>
            <everywhere/>
        </region>
        <filter>
            <material>double step:9</material>
        </filter>
        <drops>
            <item material="sandstone"/>
        </drops>
    </rule>
</block-drops>
<itemremove>
    <item>redstone</item>
    <item>redstone torch off</item>
    <item>redstone torch on</item>
    <item>seeds</item>
    <item>sapling</item>
    <item>apple</item>
    <item>golden carrot</item>
    <item>arrow</item>
    <item>log</item>
    <item>glass</item>
    <item>string</item>
    <item>web</item>
    <item>wood:2</item>
    <item>boat</item>
    <item>leather chestplate</item>
    <item>sandstone</item>
    <item>wood</item>
    <item>log</item>
    <item>golden apple</item>
    <item>ladder</item>
</itemremove>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<kill-rewards>
    <kill-reward>
        <item>golden apple</item>
        <item amount="2">arrow</item>
