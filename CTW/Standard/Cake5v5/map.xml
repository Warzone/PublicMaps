</contributors>
<teams>
    <team id="blue-team" color="blue" max="5">Blue</team>
    <team id="red-team" color="dark red" max="5">Red</team>
</teams>
<kits>
    <kit id="default">
        <item slot="0" unbreakable="true">iron sword</item>
        <item slot="1" unbreakable="true">bow</item>
        <item slot="2" unbreakable="true">diamond pickaxe</item>
        <item slot="3" unbreakable="true">iron axe</item>
        <item slot="4" amount="64">glass</item>
        <item slot="5" amount="64">sandstone</item>
        <item slot="6" amount="32">log</item>
        <item slot="7" amount="32">cooked fish</item>
        <item slot="8" amount="3">golden apple</item>
        <item slot="9" amount="32">arrow</item>
        <leggings unbreakable="true">iron leggings</leggings>
        <effect duration="5" amplifier="9">damage resistance</effect>
    </kit>
    <kit id="blue-kit" parents="default">
        <helmet unbreakable="true" color="334CB2">leather helmet</helmet>
        <chestplate unbreakable="true" color="334CB2">leather chestplate</chestplate>
        <boots unbreakable="true" color="334CB2">leather boots</boots>
    </kit>
    <kit id="red-kit" parents="default">
        <helmet unbreakable="true" color="993333">leather helmet</helmet>
        <chestplate unbreakable="true" color="993333">leather chestplate</chestplate>
        <boots unbreakable="true" color="993333">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="blue-team" kit="blue-kit" yaw="180" region="blue-spawn"/>
    <spawn team="red-team" kit="red-kit" region="red-spawn"/>
    <default yaw="-90" region="obs-spawn"/>
</spawns>
<wools craftable="false">
    <wool team="red-team" monument="blue-team-lgtblue-wool" color="light blue" location="4,14,90"/>
    <wool team="red-team" monument="blue-team-blue-wool" color="blue" location="-2,14,89"/>
    <wool team="blue-team" monument="red-team-pink-wool" color="pink" location="-4,14,-86"/>
    <wool team="blue-team" monument="red-team-red-wool" color="red" location="2,14,-85"/>
</wools>
<filters>
    <not id="not-blue">
        <team id="only-blue">blue-team</team>
    </not>
    <not id="not-red">
        <team id="only-red">red-team</team>
    </not>
    <not id="not-void">
        <void/>
    </not>
    <any id="only-web-and-water">
        <all>
            <team>red-team</team>
            <material>web</material>
        </all>
        <all>
            <team>blue-team</team>
            <material>web</material>
        </all>
        <material>water</material>
        <material>STATIONARY_WATER</material>
    </any>
</filters>
