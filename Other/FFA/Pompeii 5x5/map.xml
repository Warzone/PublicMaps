<map proto="1.5.0">
<name>Pompeii 5x5</name>
<version>1.0.4</version>
<objective>Kill as many other players as you can in 5 minutes!</objective>
<created>2016-04-10</created>
<gamemode>ffa</gamemode>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="38ab65dd-95f9-406f-917c-6accc995421f"/> <!-- GoldenOre -->
    <author uuid="5198dc37-537f-4ea5-8ee8-1e0952d4a36b"/> <!-- _3each -->
</authors>
<players min="2" max="12" colors="true"/>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" material="golden apple"/>
        <item slot="8" amount="16" material="arrow"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" material="chainmail leggings"/>
        <boots unbreakable="true" team-color="true" enchantment="protection fall:2" material="leather boots"/>
        <effect duration="2.5" amplifier="10">resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn kit="spawn-kit" spread="true" safe="true">
        <regions>
            <point yaw="-180">63.5,47,58.5</point>
            <point yaw="-80">-6,20,23.5</point>
            <point yaw="-135">-6,20,50</point>
            <point yaw="35">77,24,-41</point>
            <point yaw="-170">19,33,60</point>
            <point yaw="180">88.5,34,44.5</point>
            <point yaw="90">89,21,-4</point>
            <point>37,22,-4</point>
            <point yaw="-45">-10,23,-6</point>
        </regions>
    </spawn>
    <default yaw="180">
        <region>
            <point>61.5,22,-14.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply use="never" block="never"/>
</regions>
<score>
    <kills>1</kills>
</score>
<time>5m</time>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>arrow</item>
    <item>golden apple</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item amount="8" material="arrow"/>
    </kill-reward>
</kill-rewards>
<disabledamage>
    <damage>suffocation</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
</map>
