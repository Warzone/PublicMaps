<map proto="1.5.0">
<name>Festive FFA</name>
<version>1.0.2</version>
<objective>Kill as many players as you can in 5 minutes!</objective>
<created>2016-03-03</created>
<gamemode>ffa</gamemode>
<authors>
    <author uuid="8f9344cf-7367-4e28-9f24-5c66422f28c9"/> <!-- Timotheeee -->
</authors>
<rules>
    <rule>Fall damage is disabled.</rule>
</rules>
<players max="5" max-overfill="5" show-name-tags="false" />
<kits>
    <kit id="spawn-kit">
        <item slot="0" name="Sword" material="stone sword"/>
        <item slot="1" name="Bow" enchantment="arrow damage" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet color="E5E533" unbreakable="true" locked="true" material="leather helmet"/>
        <chestplate unbreakable="true" color="E5E533" locked="true" material="leather chestplate"/>
        <leggings color="E5E533" unbreakable="true" locked="true" material="leather leggings"/>
        <boots color="E5E533" unbreakable="true" locked="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn kit="spawn-kit" spread="true">
        <regions>
            <point yaw="0">160,2,1</point>
            <point yaw="92">143,2,15</point>
            <point yaw="-52">110,1,9</point>
            <point yaw="50">125,8,7</point>
            <point yaw="-143">111,1,36</point>
            <point yaw="-85">108,6,57</point>
            <point yaw="12">123,5,39</point>
            <point yaw="-156">141,4,54</point>
            <point yaw="-30">156,5,34</point>
            <point yaw="59">163,9,42</point>
            <point yaw="-101">151,9,57</point>
            <point yaw="43">136,13,10</point>
        </regions>
    </spawn>
    <default yaw="-90">
        <region>
            <cuboid min="136,27.5,40" max="137,27.5,38" />
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<score>
    <kills>1</kills>
</score>
<time>5m</time>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>stone sword</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
    <damage>suffocation</damage>
</disabledamage>
<timelock>on</timelock>
</map>
