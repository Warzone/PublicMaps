<map proto="1.5.0">
<name>Under the Sea</name>
<version>1.1.2</version>
<objective>Get the most kills in 5 minutes!</objective>
<gamemode>ffa</gamemode>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="38ab65dd-95f9-406f-917c-6accc995421f"/> <!-- GoldenOre -->
</authors>
<players min="2" max="5" colors="true"/>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" material="bow"/>
        <item slot="2" material="golden apple"/>
        <item slot="8" amount="16" material="arrow"/>
        <helmet locked="true" unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate locked="true" unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings locked="true" unbreakable="true" material="chainmail leggings"/>
        <boots locked="true" unbreakable="true" team-color="true" material="leather boots">
            <enchantment>feather_falling</enchantment>
        </boots>
        <effect duration="2" amplifier="255">heal</effect>
        <effect duration="2" amplifier="255">damage resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <default>
        <region yaw="-90">
            <cuboid min="-30.5,20,-30.5" max="-30.5,20,-30.5"/>
        </region>
    </default>
    <spawn kit="spawn-kit" spread="true" safe="true">
        <regions>
            <point yaw="-43">-30.5,20,-30.5</point>
            <point yaw="-20">12.5,15,-31.5</point>
            <point yaw="90">33.5,29,2.5</point>
            <point yaw="132">33.5,29,31.5</point>
            <point yaw="-90">-33.5,30,23.5</point>
            <point yaw="-83">-32.5,32,-7.5</point>
            <point yaw="-1">14.5,24,-17.5</point>
            <point yaw="-1">-1.5,24,15.5</point>
            <point yaw="90">-11.5,17,11.5</point>
        </regions>
    </spawn>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<score>
    <kills>1</kills>
</score>
<time>5m</time>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>arrow</item>
    <item>golden apple</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item amount="8" material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>suffocation</damage>
</disabledamage>
<respawn auto="true"/>
</map>
