<map proto="1.5.0">
<name>Destiny</name>
<version>1.0.2</version>
<objective>Kill as many players as you can in 5 minutes</objective>
<created>2016-03-29</created>
<gamemode>ffa</gamemode>
<include id="gapple-kill-reward"/>
<authors>
    <author uuid="e3fdaea1-8a19-4378-8ad7-41cf63f5d141"/> <!-- Avaleer -->
</authors>
<contributors>
    <contributor uuid="2ca8072f-74be-4798-85b8-bbce03aa91af" contribution="Feedback"/> <!-- Tywnis -->
    <contributor uuid="fe3608b7-d105-4029-8800-34b3147065b6" contribution="World File Help"/> <!-- rockymine -->
</contributors>
<rules>
    <rule>Kill as many players as you can in 5 minutes!</rule>
</rules>
<players max="5" max-overfill="5" show-name-tags="false"/>
<kits>
    <kit id="spawn-kit">
        <item slot="0" name="Sword" material="stone sword"/>
        <item slot="1" name="Bow" material="bow"/>
        <item slot="2" amount="10" material="arrow"/>
        <item slot="8" material="golden apple"/>
        <helmet color="167785" unbreakable="true" locked="true" material="leather helmet"/>
        <chestplate unbreakable="true" locked="true" material="iron chestplate"/>
        <leggings color="167785" unbreakable="true" locked="true" material="leather leggings"/>
        <boots color="167785" unbreakable="true" locked="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn kit="spawn-kit" spread="true">
        <regions>
            <point yaw="42">-96.5,35,82.5</point>
            <point yaw="-90">-125.5,33,100.5</point>
            <point yaw="180">-115,9,95</point>
            <point yaw="123">-83.5,24,115.5</point>
            <point yaw="45">-89.5,8,81.5</point>
        </regions>
    </spawn>
    <default yaw="-48">
        <region>
            <cuboid min="-95,34,92" max="-94,34,91"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<score>
    <kills>1</kills>
</score>
<time>5m</time>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>arrow</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item amount="3" material="arrow"/>
    </kill-reward>
</kill-rewards>
<disabledamage>
    <damage>suffocation</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
<respawn delay="1s" blackout="true" auto="true"/>
</map>
