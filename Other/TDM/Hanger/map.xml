<map proto="1.5.0">
<name>Hanger</name>
<version>1.0.7</version>
<objective>Be the first team to reach 100 points!</objective>
<gamemode>tdm</gamemode>
<authors>
    <author uuid="ba4a93aa-4935-4a71-bab2-a58f9a124e7b" contribution="Main design, building, and XML"/> <!-- ItsMiiOlly -->
    <author uuid="06078e7e-6a7b-4d31-8f74-853072b182bb" contribution="Aesthetics and WorldEdit magic"/> <!-- xXFracXx AKA Frac -->
    <author uuid="9bfbc560-244a-4e28-ab70-185f39740ec1" contribution="Aesthetics and Design"/> <!-- TheZaner -->
</authors>
<contributors>
    <contributor uuid="374988d6-4569-444e-885a-409c2f55e310" contribution="Wall design"/> <!-- Articray -->
    <contributor uuid="939a9a1c-f735-467a-bfd7-ae77cafd6693" contribution="Design and Feedback"/> <!-- PlasmaCross -->
    <contributor uuid="dee1cc51-66d2-4c5e-a8d4-403dcd1d8c83" contribution="Feedback"/> <!-- lonelyhornet -->
    <contributor uuid="a59dce2e-79e2-4229-ab83-98c885fceb23" contribution="Crate designs"/> <!-- xXNarwhaleXx -->
    <contributor uuid="dad8b95c-cf6a-44df-982e-8c8dd70201e0" contribution="XML Fixing and Testing"/> <!-- ElectroidFilms -->
</contributors>
<rules>
    <rule>Fall damage is disabled!</rule>
    <rule>Do not block glitch into unreachable places!</rule>
    <rule>Spawn camping is not allowed, not to mention you'll get owned.</rule>
</rules>
<teams>
    <team id="blue" color="blue" max="15" max-overfill="20">Blue</team>
    <team id="yellow" color="yellow" max="15" max-overfill="20">Yellow</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" name="`c`lKnife" unbreakable="true" enchantment="damage all" material="iron sword"/>
        <item slot="1" name="`c`lGun" unbreakable="true" enchantment="arrow infinite" material="bow"/>
        <item slot="28" material="arrow"/>
        <item slot="2" name="`c`lHealth Kit" damage="8229" material="potion"/>
        <helmet team-color="true" unbreakable="true" material="leather helmet"/>
        <chestplate unbreakable="true" material="iron chestplate"/>
        <leggings team-color="true" unbreakable="true" material="leather leggings"/>
        <boots team-color="true" unbreakable="true" material="leather boots"/>
        <effect duration="7" amplifier="25">heal</effect>
        <effect duration="7" amplifier="25">damage resistance</effect>
        <effect>speed</effect>
        <effect amplifier="8">jump</effect>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="spawn-kit" yaw="90">
        <region>
            <cylinder base="-10,1,733" radius="3" height="0"/>
        </region>
    </spawn>
    <spawn team="yellow" kit="spawn-kit" yaw="-90">
        <region>
            <cylinder base="-101,1,733" radius="3" height="0"/>
        </region>
    </spawn>
    <default yaw="180">
        <region>
            <cylinder base="-55,18,733" radius="1" height="0"/>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<time>15m</time>
<score>
    <limit>100</limit>
</score>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>glass bottle</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>potion</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item name="`c`lHealth Kit" damage="8229" material="potion"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
<modifybowprojectile>
    <velocityMod>2.0</velocityMod>
</modifybowprojectile>
<timelock>on</timelock>
</map>
