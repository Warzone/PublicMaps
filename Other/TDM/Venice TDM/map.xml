</contributors>
<teams>
    <team id="blue" color="blue" max="40">Blue</team>
    <team id="red" color="dark red" max="40">Red</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <clear/>
        <item slot="0" unbreakable="true" material="stone sword"/>
        <item slot="1" unbreakable="true" enchantment="arrow infinite" material="bow"/>
        <item slot="2" unbreakable="true" enchantment="dig speed" material="iron pickaxe"/>
        <item slot="3" material="golden apple"/>
        <item slot="4" amount="8" material="wood"/>
        <item slot="5" amount="16" material="ladder"/>
        <item slot="6" material="boat"/>
        <item slot="28" material="arrow"/>
        <helmet unbreakable="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" enchantment="protection projectile:3" material="chainmail leggings"/>
        <boots unbreakable="true" enchantment="protection fall:2" material="iron boots"/>
        <effect duration="5">heal</effect>
        <effect duration="5">damage resistance</effect>
        <effect>speed</effect>
    </kit>
</kits>
<spawns>
    <spawn team="blue" kit="spawn-kit">
        <region>
            <cuboid min="7,12,-84" max="-6,12,-76"/>
        </region>
    </spawn>
    <spawn team="red" kit="spawn-kit" yaw="180">
        <region>
            <cuboid min="-6,12,85" max="7,12,77"/>
        </region>
    </spawn>
    <default yaw="315">
        <region>
            <cylinder base="-104,58,-111" radius="5" height="0"/>
        </region>
    </default>
</spawns>
<time>10m</time>
<score>
    <kills>1</kills>
</score>
