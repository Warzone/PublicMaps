<map proto="1.5.0">
<name>Rig</name>
<version>1.0.3</version>
<objective>Eliminate the opposing team!</objective>
<gamemode>arcade</gamemode>
<gamemode>br</gamemode>
<authors>
    <author uuid="2a289d2a-d970-49c5-9a6c-01fc0264e317"/> <!-- StealthMedia -->
</authors>
<contributors>
    <contributor uuid="bb628049-e368-4b1c-b8aa-d4557ae9416b" contribution="XML"/> <!-- Anthonny -->
</contributors>
<teams>
    <team id="yellow" color="yellow" max="10" max-overfill="12">Yellow</team>
    <team id="orange" color="gold" max="10" max-overfill="12">Orange</team>
</teams>
<kits>
    <kit id="spawn-kit">
        <item slot="0" enchantment="damage all:10" unbreakable="true" material="iron sword"/>
        <item slot="1" enchantment="arrow damage:10" unbreakable="true" material="bow"/>
        <item slot="2" material="arrow"/>
        <helmet unbreakable="true" locked="true" team-color="true" material="leather helmet"/>
        <chestplate unbreakable="true" locked="true" team-color="true" material="leather chestplate"/>
        <leggings unbreakable="true" locked="true" team-color="true" material="leather leggings"/>
        <boots unbreakable="true" locked="true" team-color="true" material="leather boots"/>
        <game-mode>adventure</game-mode>
    </kit>
</kits>
<spawns>
    <spawn team="yellow" kit="spawn-kit">
        <region>
            <cuboid min="-17,15,-21" max="-15,15,-20"/>
        </region>
    </spawn>
    <spawn team="orange" kit="spawn-kit">
        <region yaw="180">
            <cuboid min="18,15,21" max="20,15,22"/>
        </region>
    </spawn>
    <default>
        <region>
            <point>1.5,43,0.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never"/>
</regions>
<blitz>
    <lives>1</lives>
</blitz>
<rage/>
<time>5m</time>
<itemremove>
    <item>iron sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-rewards>
    <kill-reward>
        <item material="arrow"/>
    </kill-reward>
</kill-rewards>
<hunger>
    <depletion>off</depletion>
</hunger>
<disabledamage>
    <damage>fall</damage>
</disabledamage>
</map>
