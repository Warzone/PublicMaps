<map proto="1.4.1">
<name>Stonehenge FFA</name>
<version>1.2.3</version>
<created>2016-04-24</created>
<authors>
    <author uuid="7ab24d14-0801-48ba-8fea-147ddf400067"/>
    <!-- samthecat43 -->
    <author uuid="a43135a4-d7b9-49ab-ac8d-a16bb2513c8d"/>
    <!-- Ny_ -->
</authors>
<contributors>
    <contributor uuid="670223bb-7560-48c8-8f01-2f463549b917" contribution="he's amazin"/>
    <!-- twiz_mwazin -->
</contributors>
<objective>Be the player with the most kills after 5 minutes!</objective>
<gamemode>ffa</gamemode>
<time>5m</time>
<score>
    <kills>1</kills>
</score>
<players min="2" max="5" colors="true"/>
<kits>
    <kit id="player">
        <clear/>
        <item slot="0" unbreakable="true">stone sword</item>
        <item slot="1" unbreakable="true" enchantment="arrow infinite:1">bow</item>
        <item slot="2">golden apple</item>
        <item slot="28">arrow</item>
        <helmet unbreakable="true" team-color="true">leather helmet</helmet>
        <chestplate unbreakable="true" team-color="true">leather chestplate</chestplate>
        <leggings unbreakable="true">chainmail leggings</leggings>
        <boots unbreakable="true" team-color="true" enchantment="protection fall:2">leather boots</boots>
        <potion duration="2.5" amplifier="10">resistance</potion>
    </kit>
    <kit id="obs">
        <potion duration="oo">night vision</potion>
    </kit>
</kits>
<spawns>
    <spawn kit="player" spread="true" safe="true">
        <regions angle="1,13,-1">
            <cylinder base="1,13,-1" radius="70" height="20"/>
        </regions>
    </spawn>
    <default kit="obs">
        <regions angle="1,13,-1">
            <point yaw="0">-9.5,18,-45.5</point>
        </regions>
    </default>
</spawns>
<killreward>
    <item>golden apple</item>
</killreward>
<toolrepair>
    <tool>stone sword</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>chainmail leggings</item>
    <item>leather boots</item>
    <item>golden apple</item>
</itemremove>
<hunger>
    <depletion>off</depletion>
</hunger>
<timelock>on</timelock>
</map>
