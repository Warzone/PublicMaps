<map proto="1.4.0">
<name>Rainbow FFA</name>
<version>1.0.0</version>
<objective>Kill as many other players as you can in 3 minutes!</objective>
<gamemode>ffa</gamemode>
<authors>
    <author uuid="bbfac3db-dd46-4996-b936-0e2432f33285" />
    <!--    Valkyh    -->
</authors>
<score>
    <kills>1</kills>
</score>
<time>3m</time>
<kits>
    <kit id="spawn-kit">
        <item slot="0" unbreakable="true" locked="true">stone sword</item>
        <item slot="1" unbreakable="true" locked="true">bow</item>
        <item slot="8" amount="8">arrow</item>
        <effect duration="1s" amplifier="10">resistance</effect>
        <chestplate unbreakable="true" locked="true">gold chestplate</chestplate>
        <boots color="000000" unbreakable="true" locked="true">leather boots</boots>
    </kit>
</kits>
<players min="2" max="8" max-overfill="8" show-name-tags="false" />
<spawns>
    <spawn kit="spawn-kit" spread="true">
        <regions>
            <point yaw="0">0.5,3.5,-24.5</point>
			<point yaw="45">19.5,3.5,-18.5</point>
			<point yaw="90">25.5,3.5,0.5</point>
			<point yaw="135">19.5,3.5,19.5</point>
			<point yaw="180">0.5,3.5,25.5</point>
			<point yaw="-135">-18.5,3.5,19.5</point>
			<point yaw="-90">-24.5,3.5,0.5</point>
			<point yaw="-45">-18.5,3.5,-18.5</point>
        </regions>
    </spawn>
    <default>
        <region>
            <point yaw="180">0.5,6,0.5</point>
        </region>
    </default>
</spawns>
<regions>
    <apply block="never" />
</regions>
<hunger>
    <depletion>off</depletion>
</hunger>
<respawn delay="3s" blackout="true" />
<killreward>
        <item amount="4">arrow</item>
        <item>golden apple</item>
</killreward>
<disabledamage>
    <damage>suffocation</damage>
    <damage>fall</damage>
</disabledamage>
<itemremove>
    <item>stone sword</item>
    <item>bow</item>
    <item>gold chestplate</item>
    <item>leather boots</item>
    <item>arrow</item>
</itemremove>
<timelock>on</timelock>
</map>
