<map proto="1.4.2">
<name>Warlock CTF</name>
<version>1.4.4</version>
<objective>Capture the enemy's flag 3 times to win!</objective>
<gamemode>ctf</gamemode>
<authors>
    <author uuid="e5953ddf-c1fc-4405-9ac9-6939631cd185"/> <!-- McSpider -->
</authors>
<contributors>
    <contributor uuid="ef4ea031-998f-4ec9-b7b6-1bdd428bcef8" contribution="Map Feedback and XML Coding"/> <!-- Plastix -->
</contributors>
<teams>
    <team id="red" color="dark red" max="13">Red</team>
    <team id="blue" color="blue" max="13">Blue</team>
</teams>
<kits>
    <kit id="spawn">
        <item slot="0" unbreakable="true" material="iron sword"/>
        <item slot="1" enchantment="arrow infinite:1" unbreakable="true" material="bow"/>
        <item slot="28" material="arrow"/>
        <item slot="2" unbreakable="true" material="diamond pickaxe"/>
        <item slot="3" unbreakable="true" material="iron axe"/>
        <item slot="5" amount="8" material="glass"/>
        <item slot="6" amount="16" damage="2" material="wood"/>
        <item slot="17" amount="2" material="gold nugget"/>
        <item slot="8" amount="64" material="cooked beef"/>
        <item slot="27" amount="64" material="apple"/>
        <potion duration="2" amplifier="1">heal</potion>
        <potion duration="4">damage resistance</potion>
    </kit>
    <kit id="red-spawn" parents="spawn">
        <helmet color="cd0000" unbreakable="true" material="leather helmet"/>
        <chestplate color="cd0000" enchantment="protection projectile:2" unbreakable="true" material="leather chestplate"/>
        <leggings color="cd0000" unbreakable="true" material="leather leggings"/>
        <boots color="cd0000" unbreakable="true" material="leather boots"/>
    </kit>
    <kit id="blue-spawn" parents="spawn">
        <helmet color="0066cc" unbreakable="true" material="leather helmet"/>
        <chestplate color="0066cc" enchantment="protection projectile:2" unbreakable="true" material="leather chestplate"/>
        <leggings color="0066cc" unbreakable="true" material="leather leggings"/>
        <boots color="0066cc" unbreakable="true" material="leather boots"/>
    </kit>
</kits>
<spawns>
    <spawn kit="red-spawn" team="red" yaw="0">
        <region>
            <cuboid min="40,6,-30" max="44,6,-34"/>
        </region>
    </spawn>
    <spawn kit="blue-spawn" team="blue" yaw="180">
        <region>
            <cuboid min="40,6,31" max="44,6,35"/>
        </region>
    </spawn>
    <default yaw="90">
        <region>
            <point>133.5,22,0.5</point>
        </region>
    </default>
</spawns>
<filters>
    <team id="only-red">red</team>
    <team id="only-blue">blue</team>
    <deny id="no-beacon">
        <material>beacon</material>
    </deny>
</filters>
<regions>
    <!-- Definitions -->
    <union id="spawn-protections">
        <union id="red-base">
            <cuboid min="36,5,-38" max="55,11,-25"/>
            <cuboid min="54,5,-31" max="61,11,-38"/>
        </union>
        <union id="blue-base">
            <cuboid min="36,5,39" max="55,11,26"/>
            <cuboid min="54,5,32" max="61,11,39"/>
        </union>
    </union>
    <union id="flag-platform">
        <cylinder base="77.5,4,-36.5" radius="5" height="oo"/>
        <cylinder base="77.5,4,37.5" radius="5" height="oo"/>
    </union>
    <negative id="negative">
        <cuboid min="55,40,-oo" max="60,-oo,oo"/>
        <cuboid min="-oo,-oo,-oo" max="121,22,oo"/>
    </negative>
    <!-- Applications -->
    <apply region="spawn-protections" block="never" message="You are not allowed to modify terrain here."/>
    <apply region="flag-platform" block="never" message="You may not edit the flag platform"/>
    <apply region="negative" block="never"/>
    <apply use="no-beacon"/>
</regions>
<flags flag-proximity-metric="none" net-proximity-metric="none">
    <!-- Blue Flag -->
    <flag id="blue-flag" name="Blue Flag" owner="blue" color="blue" pickup-filter="only-red">
        <post return-time="20s" yaw="180">77.5,6,37.5</post>
    </flag>
    <net points="1" flag="blue-flag" return="red-flag">
        <region>
            <cylinder base="77.5,5,-36.5" radius="3" height="3"/>
        </region>
    </net>
    <!-- Red Flag -->
    <flag id="red-flag" name="Red Flag" owner="red" color="red" pickup-filter="only-blue">
        <post return-time="20s" yaw="0">77.5,6,-36.5</post>
    </flag>
    <net points="1" flag="red-flag" return="blue-flag">
        <region>
            <cylinder base="77.5,5,37.5" radius="3" height="3"/>
        </region>
    </net>
</flags>
<score>
    <kills>0</kills>
    <deaths>0</deaths>
    <limit>3</limit>
</score>
<killreward>
    <item>gold nugget</item>
    <item>golden apple</item>
</killreward>
<tnt>
    <instantignite>on</instantignite>
    <licensing>off</licensing>
    <friendly-defuse>off</friendly-defuse>
</tnt>
<toolrepair>
    <tool>iron sword</tool>
    <tool>diamond pickaxe</tool>
    <tool>iron axe</tool>
    <tool>bow</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
    <item>wood</item>
    <item>glass</item>
    <item>apple</item>
    <item>cooked beef</item>
    <item>obsidian</item>
    <item>beacon</item>
    <item>golden apple</item>
</itemremove>
</map>