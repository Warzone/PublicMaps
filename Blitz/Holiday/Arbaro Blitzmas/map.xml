<?xml version="1.0"?>
<map proto="1.3.3">
<name>Arbaro Blitzmas</name>
<version>1.0.0</version>
<objective>Eliminate the other team within 5 minutes!</objective>
<gamemode>blitz</gamemode>
<authors>
    <author uuid="7a150842-5db4-487e-b4d1-559b8240a07f"/> <!-- carterpaul -->
    <author uuid="4a68c267-51d1-4320-8ed0-dd85f48cb8a9"/> <!-- PenguinsCubed -->
</authors>
<contributors>
    <contributor uuid="083ec416-c87e-438f-8732-170798b4fe93" contribution="XML"/> <!-- orky48 -->
</contributors>
<teams>
    <team color="dark red" max="16">Red</team>
    <team color="dark green" max="16">Green</team>
</teams>
<blitz>
    <lives>1</lives>
    <broadcastLives>true</broadcastLives>
    <time>5m</time>
</blitz>
<timelock>on</timelock>
<kits>
    <kit name="spawn">
        <item slot="0">iron axe</item>
        <item slot="1">bow</item>
        <item slot="7" amount="16">arrow</item>
        <item slot="2" amount="64">golden carrot</item>
        <item slot="8">golden apple</item>
        <chestplate enchantment="protection projectile:2">chainmail chestplate</chestplate>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="992114">leather helmet</helmet>
        <leggings color="992114">leather leggings</leggings>
        <boots color="992114" enchantment="protection fall:1">leather boots</boots>
    </kit>
        <kit name="green" parents="spawn">
        <helmet color="1A3112">leather helmet</helmet>
        <leggings color="1A3112">leather leggings</leggings>
        <boots color="1A3112" enchantment="protection fall:1">leather boots</boots>
    </kit>
</kits>
<spawns>
    <spawn team="red" yaw="-90" kit="red">
        <cylinder base="-0,42,1" radius="2" height="0"/>
    </spawn>
    <spawn team="green" yaw="90" kit="green">
        <cylinder base="113,42,0" radius="2" height="0"/>
    </spawn>
    <default yaw="-0">
        <cylinder base="55,73,58" radius="1" height="0"/>
    </default>
</spawns>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
    <apply enter="deny-all" message="This part of the map is disabled.">
        <cuboid min="-oo,52,-oo" max="oo,54,oo"/>
    </apply>
</regions>
<itemremove>
    <item>arrow</item>
    <item>golden carrot</item>
    <item>leather helmet</item>
    <item>chainmail chestplate</item>
    <item>leather boots</item>
    <item>leather leggings</item>
    <item>golden apple</item>
</itemremove>
<toolrepair>
    <tool>iron axe</tool>
    <tool>bow</tool>
</toolrepair>
<killreward>
    <item>golden apple</item>
    <item amount="8">arrow</item>
</killreward>
</map>
