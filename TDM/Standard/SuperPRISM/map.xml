<map proto="1.4.0">
<name>SuperPRISM</name>
<version>1.3.5</version>
<objective>Be the team with the most points after 5 minutes!</objective>
<gamemode>tdm</gamemode>
<gamemode>rage</gamemode>
<gamemode>arcade</gamemode>
