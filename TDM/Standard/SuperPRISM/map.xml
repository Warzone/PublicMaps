<item-mods>
    <rule>
        <match>
            <material>tnt</material>
        </match>
        <modify show-can-place-on="false">
            <can-place-on>
                <all-blocks/>
            </can-place-on>
        </modify>
    </rule>
</item-mods>
<portals>
    <portal x="@-1119" y="@1" z="@-2153">
        <region>
            <cuboid min="-1039,1,-2179" max="-1038,6,-2178"/>
        </region>
    </portal>
    <portal x="@-1045" y="@1" z="@-2179">
        <region>
            <cuboid min="-1125,1,-2153" max="-1124,6,-2152"/>
        </region>
    </portal>
</portals>
<itemremove>
    <item>gold sword</item>
    <item>bow</item>
    <item>arrow</item>
    <item>tnt</item>
    <item>glass bottle</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
<kill-reward>
    <item amount="2">arrow</item>
</kill-reward>
<score>
    <kills>1</kills>
</score>
<time>7m</time>
<rage/>
<respawn auto="true"/>
<modifybowprojectile>
    <effect duration="2" amplifier="1">blindness</effect>
</modifybowprojectile>
<disabledamage>
    <damage>fall</damage>
    <damage ally="true" self="true" enemy="false" other="false">block explosion</damage>
</disabledamage>
<tnt>
    <instantignite>on</instantignite>
    <licensing>off</licensing>
    <friendly-defuse>off</friendly-defuse>
    <blockdamage>off</blockdamage>
</tnt>
<hunger>
    <depletion>off</depletion>
</hunger>
</map>
